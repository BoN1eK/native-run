{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "declaration": true,
    "importHelpers": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "outDir": "./dist",
    "pretty": true,
    "strict": true,
    "target": "es2018",
    "lib": [
      "es2018"
    ]
  }
}
