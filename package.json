{
  "name": "native-run",
  "version": "0.0.0",
  "description": "A CLI for running apps on iOS/Android devices and simulators/emulators",
  "bin": {
    "native-run": "./bin/native-run"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "precommit": "npm run lint"
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">=8.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/native-run.git"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/node": "~8.9.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-ionic-rules": "0.0.17",
    "typescript": "^3.0.1"
  },
  "keywords": [
    "android",
    "ios",
    "cli",
    "mobile",
    "app",
    "hybrid",
    "native"
  ],
  "author": "Ionic Team <hi@ionic.io> (https://ionicframework.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ionic-team/native-run/issues"
  },
  "homepage": "https://github.com/ionic-team/native-run#readme",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
